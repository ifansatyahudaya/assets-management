<div class="page-header">
  <%= link_to transactions_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-all"></span>
    Back
  <% end %>
  <%= link_to edit_transaction_path(@transaction), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show Transaction</h1>
</div>

<dl class="dl-horizontal">
  <dt>Titlte of Transaction:</dt>
  <dd><%= @transaction.title %></dd>  

  <dt>Date of Transaction:</dt>
  <dd><%= @transaction.date_transaction %></dd>

  <dt>Price:</dt>
  <dd><%= @transaction.price %></dd>
</dl>